{% extends "admin/base" %}

{% block title %}Posts &mdash; {% endblock title %}

{% block content %}

<h2>Posts</h2>

<a href="/admin/posts/new">New Post</a>

<ul>
  {% for post in posts %}
  <li>
    <a href="/admin/posts/edit/{{ post.id }}">{{ post.title }}</a>
  </li>
  {% endfor %}
</ul>

{% endblock content %}